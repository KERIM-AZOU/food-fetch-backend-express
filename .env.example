# Server Configuration
FETCH_PORT=5006

# Groq API — transcription (Whisper) and chat (Llama)
# https://console.groq.com
GROQ_API_KEY=your_groq_api_key_here

# ElevenLabs API — text-to-speech (fallback when Gemini disabled)
# https://elevenlabs.io
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# Google Gemini / Vertex AI — native audio, TTS, extraction
# https://aistudio.google.com
GEMINI_API_KEY=your_gemini_api_key_here

# Toggle AI provider: 'true' = Gemini, 'false' = Groq/ElevenLabs
USE_GEMINI=true
